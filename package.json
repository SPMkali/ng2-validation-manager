{
  "name": "ng2-validation-manager",
  "version": "0.1.3",
  "description": "Your library description.",
  "main": "ng2-validation-manager",
  "typings": "ng2-validation-manager.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabrio/ng2-validation-manager.git"
  },
  "keywords": [
    "angular2",
    "ng",
    "ng2",
    "validation",
    "library",
    "laravel",
    "validate",
    "form builder"
  ],
  "author": "Sabri Aziri <azirisabri@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabrio/ng2-validation-manager/issues"
  },
  "homepage": "https://github.com/sabrio/ng2-validation-manager#readme",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "tsc --p tsconfig-build.json",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"example/**/*.ts\"",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start",
    "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json example/app/app.component.ts",
    "start": "npm run server",
    "start:hmr": "npm run server -- --hot",
    "postinstall": "npm run webdriver-update"
  },
  "dependencies": {},
  "devDependencies": {}
}
